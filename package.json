{
  "name": "@apset/jupyterlab-cognos-dashboard-embedded",
  "version": "1.0.2",
  "description": "Cognos Dashboard Embedded for JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "MIT",
  "author": "James Reeve",
  "homepage": "https://github.ibm.com/cognitive-class-labs/jupyterlab-cognos-dashboard-embedded",
  "bugs": {
    "url": "https://github.ibm.com/cognitive-class-labs/jupyterlab-cognos-dashboard-embedded/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/cognitive-class-labs/jupyterlab-cognos-dashboard-embedded"
  },
  "publishConfig": {
    "registry": "https://na.artifactory.swg-devops.com/artifactory/api/npm/apset-npm/"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo 'not yet implemented'",
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "tslint": "tslint",
    "precommit": "lint-staged",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "^1.0.0",
    "@jupyterlab/coreutils": "^3.0.0",
    "@jupyterlab/docmanager": "^1.0.0",
    "@jupyterlab/docregistry": "^1.0.0",
    "@jupyterlab/filebrowser": "^1.0.0",
    "@jupyterlab/launcher": "^1.0.0",
    "@jupyterlab/services": "^4.0.0",
    "@phosphor/coreutils": "^1.3.1",
    "@phosphor/messaging": "^1.2.3",
    "@phosphor/widgets": "^1.8.1",
    "@types/jquery": "^3.3.2",
    "@types/react-dom": "^16.8.2",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyterlab_cognos_dashboard_embedded"
        }
      }
    }
  }
}
